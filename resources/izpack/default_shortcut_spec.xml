<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- This is the shortcut specification for windows. Its considered default 
	so on Systems that are not shortcut compatible the information is taken from 
	here. -->
<shortcuts>
	<shortcut name="esupSgcClient"
		target="$INSTALL_PATH\java\bin\javaw.exe" commandLine="-Dcom.sun.webkit.useHTTP2Loader=false -jar esupsgcclient-shib.jar"
		workingDirectory="$INSTALL_PATH\$version" description="esupsgclient"
		iconFile="$INSTALL_PATH\icon\esupsgcclient.ico" iconIndex="0"
		initialState="maximized" desktop="yes"
		applications="yes" startMenu="yes" />
	<shortcut name="esupNfcTagKeyboard"
			  target="$INSTALL_PATH\java\bin\javaw.exe" commandLine="--add-exports java.desktop/com.sun.java.swing.plaf.windows=ALL-UNNAMED -jar esupnfctagkeyboard-shib.jar"
			  workingDirectory="$INSTALL_PATH\$version" description="esupnfctagkeyboard"
			  iconFile="$INSTALL_PATH\icon\esupnfctagkeyboard.ico" iconIndex="0"
			  initialState="maximized" desktop="yes"
			  applications="yes" startMenu="yes" />
	<shortcut name="esupNfcTagDesktop"
			  target="$INSTALL_PATH\java\bin\javaw.exe" commandLine="-Dcom.sun.webkit.useHTTP2Loader=false -jar esupnfctagdesktop-shib.jar"
			  workingDirectory="$INSTALL_PATH\$version" description="esupnfctagdesktop"
			  iconFile="$INSTALL_PATH\icon\esupnfctagdesktop.ico" iconIndex="0"
			  initialState="normal" desktop="yes"
			  applications="yes" startMenu="yes" />
</shortcuts>
